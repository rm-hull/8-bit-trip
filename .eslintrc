{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "mocha": true
  },
  "settings": {
    "ecmascript": 6,
    "jsx": true
  },
  "plugins": [
    "react"
  ],
  "globals": {
    "require": true,
    "module": true
  },
  "rules": {
    "strict": 0,
    "curly": 1,
    "quotes": 0,
    "camelcase": 0,
    "comma-dangle": 1,
    "no-use-before-define": 2,
    "no-unused-vars": 2,
    "no-mixed-requires": [1, true],
    "max-depth": [1, 5],
    "eqeqeq": 1,
    "no-path-concat": 0,
    "no-else-return": 1,
    "no-eq-null": 1,
    "no-lonely-if": 1,
    "new-cap": 0,
    "eol-last": 0,
    "comma-spacing": 0,
    "key-spacing": 0,
    "no-multi-spaces": 1,
    "handle-callback-err": [2, "^err\\d*" ],
    "react/jsx-no-undef": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/react-in-jsx-scope": 1,
    "react/no-danger": 1,
    "react/no-deprecated": 1
  }
}
